{
  "name": "node-api-boilerplate",
  "version": "1.0.0",
  "description": "A REST & GraphQL API Node.js Boilerplate",
  "main": "build/index.js",
  "repository": "git@github.com:alaister/node-api-boilerplate.git",
  "author": "Alaister Young <a@alaisteryoung.com>",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=11.0.0"
  },
  "scripts": {
    "start": "node ./build/index.js",
    "dev": "nodemon --exec node -r 'dotenv/config' -r '@babel/register' ./src/index.js",
    "build": "ncc build ./src/index.js -o build",
    "migrate": "knex migrate:latest"
  },
  "dependencies": {
    "cuid": "^2.1.4",
    "dataloader": "^1.4.0",
    "graphql": "^14.0.2",
    "graphql-api-koa": "^2.0.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-playground-middleware-koa": "^1.6.8",
    "graphql-relay": "^0.5.5",
    "graphql-type-json": "^0.2.1",
    "graphql-upload": "^8.0.4",
    "knex": "^0.16.3",
    "koa": "^2.6.2",
    "koa-bodyparser": "^4.2.1",
    "koa-passport": "^4.1.1",
    "koa-router": "^7.4.0",
    "koa-session": "^5.10.1",
    "objection": "^1.4.0",
    "objection-guid": "^3.0.2",
    "objection-password": "^2.0.0",
    "objection-soft-delete": "^1.0.5",
    "objection-unique": "^1.0.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.7.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/register": "^7.0.0",
    "@zeit/ncc": "^0.9.0",
    "babel-eslint": "^10.0.1",
    "dotenv": "^6.2.0",
    "eslint": "^5.12.0",
    "eslint-config-env": "^2.0.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-import-order-alphabetical": "0.0.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3"
  }
}
